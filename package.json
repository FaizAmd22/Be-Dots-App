{
   "name": "backend",
   "version": "0.0.1",
   "description": "Server of Dots App",
   "type": "commonjs",
   "main": "dist/index.js",
   "devDependencies": {
      "@types/bcrypt": "^5.0.2",
      "@types/cors": "^2.8.17",
      "@types/express": "^4.17.21",
      "@types/multer": "^1.4.11",
      "@types/node": "^16.11.10",
      "ts-node": "10.9.1",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "bcrypt": "^5.1.1",
      "body-parser": "^1.20.2",
      "cloudinary": "^2.0.1",
      "cors": "^2.8.5",
      "dotenv": "^16.4.2",
      "express": "^4.18.2",
      "joi": "^17.12.1",
      "jsonwebtoken": "^9.0.2",
      "multer": "^1.4.5-lts.1",
      "nodemon": "^3.0.3",
      "path": "^0.12.7",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.20"
   },
   "scripts": {
      "start": "node dist/index.js",
      "dev": "nodemon --exec ts-node src/index.ts",
      "build": "tsc",
      "typeorm": "typeorm-ts-node-commonjs",
      "worker": "nodemon --exec ts-node src/workers/index.ts",
      "migrate:generate": "npx typeorm-ts-node-commonjs migration:generate ./src/migrations/My-Migration -d ./src/data-source.ts",
      "migrate:run": "npx typeorm-ts-node-commonjs migration:run -d ./src/data-source.ts",
      "schema:drop": "npx typeorm-ts-node-commonjs schema:drop -d ./src/data-source.ts"
   }
}
